<%-include("../../partials/header")%>

<div class="col-12 grid-margin mb-4">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Earnings History</h4>
    
                <%if(history.length){%>
                    <div class="table-responsive">
                        <table id="mytable" class="table">
                            <thead>
                                <tr>
                    
                                    <th> # </th>
                                    <th> Details </th>
                                    <th> Amount </th>
                                    <th> Date </th>
                    
                                </tr>
                            </thead>
                            <tbody>
                                <%history.map((eh,index)=>{%>
                    
                                    <tr>
                                        <td>
                                            <%=index + 1%>
                                        </td>
                    
                                        <td>
                                            <%=eh.h_text%>
                                        </td>
                    
                                        <td> <span class="text-xs font-weight-bold">
                                                <%=eh.h_amount%> <span style="font-size: 12px;"><%=websiteDetails.website_reward_name%></span>
                                            </span> </td>
                    
                                        <td>
                                            <%=eh.h_created_at%>
                                        </td>
                    
                                    </tr>
                    
                                    <%})%>
                    
                    
                            </tbody>
                        </table>
                        <div class="d-flex justify-content-center">
                            <%-include("../../partials/pagination")%>
                    
                        </div>
                    </div>
                    <%}else{%>
                        <h6 class="text-center pt-3">No Earning Stats At The Moment</h6>
                        <%}%>
    
    
            </div>
        </div>
    </div>


<%-include("../../partials/footer")%>