<%- include('../../partials/header'); %>




    <div class="container-fluid dashboard-content">

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box">
                    <h4 class="page-title">
                        <%= title %>
                    </h4>
                </div>
            </div>
        </div>
        <!-- end page title -->


        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <h5 class="card-header">Generation List 
                        
                        <div class="actions float-right">
                    
                    
                        <a data-toggle="modal" data-target="#new" href="javascript: void(0);" id="new-method"
                            class="btn btn-xs btn-info ml-1 pay">New Generation <i class="fa fa-plus-circle" aria-hidden="true"></i></a>
                    </div>
                    </h5>
                    <div class="card-body p-0">
                        <%if(generation.length> 0){ %>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead class="bg-light">
                                        <tr class="border-0">
                                            <th class="border-0">#</th>
                                            <th class="border-0">Name</th>
                                            <th class="border-0">Percentage</th>
                                            <th class="border-0">Plan Subscribe Percent</th>
                                            <th class="border-0">Action</th>
        
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%generation.map((gn,index)=>{ %>
                                            <tr>
                                                <td>
                                                    <%=index + 1 %>
                                                </td>
        
                                                <td>
                                                    <%=gn.g_name %>
                                                </td>
                                                <td>
                                                    <%=gn.g_percent %> %
                                                </td>
                                                <td>
                                                    <%=gn.g_plan_percent %> %
                                                </td>
                                                
        
                                                <td>
                                                    <a 
                                                    href="javascript: void(0);" class="btn btn-xs btn-info ml-1 edit" data-toggle="modal" data-target="#edit-<%=gn.g_id %>"><i
                                                            class="fa fa-pencil" aria-hidden="true"></i></a>
        
                                                    <a  data-id="<%=gn.g_id %>" href="javascript: void(0);" class="btn btn-xs btn-danger ml-1 delete-package"><i
                                                            class="fa fa-trash" aria-hidden="true"></i></a>
                                                </td>
        
        
                                            </tr>

                                            <%-include("./modal/editModal",{gn})%>
                                            <% }) %>
        
                                                <tr>
                                                    <td colspan="9"></td>
                                                </tr>
                                    </tbody>
        
                                </table>
        
                                
        
                            </div>
                            <% }else{%>
                                <h3 class="text-center pt-3">No Generation At The Moment</h3>
                                <%}%>
                    </div>
                </div>
            </div>
        </div>



        
    </div>
<%-include("./modal/newModal")%>
<script src="/admin/js/generation/generation.js"></script>
<%-include('../../partials/footer'); %>